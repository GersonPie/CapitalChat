{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "functions": {
      "api": {
        "handler": "index.handler",
        "runtime": "nodejs14.x",
        "memory": 1024
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, OPTIONS, PUT, DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          }
        ]
      }
    ]
  }
  